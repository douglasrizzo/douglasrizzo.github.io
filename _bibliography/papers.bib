@article{Travitzki2021,
  title = {{Teste adaptativo informatizado da Provinha Brasil {\textendash}
           Leitura: resultados e perspectivas}},
  shorttitle = {{Teste adaptativo informatizado da Provinha Brasil {\textendash}
                Leitura}},
  author = {Travitzki, Rodrigo and Alavarse, Ocimar Munhoz and Meneghetti,
            Douglas De Rizzo and Catalani, {\'E}rica Maria de Toledo},
  year = {2021},
  month = feb,
  journal = {Estudos em Avalia{\c c}{\~a}o Educacional},
  volume = {31},
  number = {78},
  pages = {525--553},
  issn = {1984-932X},
  doi = {10/gjjct7},
  urldate = {2021-02-19},
  abstract = {Este artigo descreve um Teste Adaptativo Informatizado (TAI) para
              a Provinha Brasil {\textendash} Leitura, com base na Teoria da
              Resposta ao Item. Detalha-se o funcionamento e processo de
              desenvolvimento do algoritmo. O TAI foi aplicado com o uso de
              tablets a 1.983 alunos dos 1o e 2o anos do ensino fundamental, em
              15 escolas da Rede Municipal de Ensino de S{\~a}o Paulo. Os
              resultados confirmam a qualidade dos itens da Provinha Brasil, do
              trabalho realizado nas escolas e, sobretudo, do TAI. Em rela{\c c}{
              \~a}o {\`a} gest{\~a}o do tempo de prova, conclui-se que h{\'a} uma
              associa{\c c}{\~a}o de mesmo sentido entre profici{\^e}ncia e tempo
              , mas s{\'o} at{\'e} certo ponto; os alunos tendem a demorar mais
              nos itens mais dif{\'i}ceis; essa tend{\^e}ncia {\'e} mais intensa
              nos alunos mais proficientes, confirmando a hip{\'o}tese de que
              eles tendem a gerir melhor o tempo de prova.},
  copyright = {Copyright (c) 2021 Estudos em Avalia{\c c}{\~a}o Educacional},
  langid = {portuguese},
  keywords = {Teoria da Resposta ao Item},
  file = {/home/dodo/Documents/Bibliografia/Doutorado/Travitzki 2021 - Teste
          adaptativo informatizado da Provinha Brasil – Leitura - resultados
          e.pdf},
}
